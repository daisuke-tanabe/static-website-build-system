{
  "name": "webpack-jquery-static-website-boiler-template",
  "version": "0.0.0",
  "description": "jqueryの使用を前提とした静的ウェブサイトを作成するためのボイラーテンプレート",
  "engines": {
    "node": "10.x.x"
  },
  "scripts": {
    "dev": "run-s cpx serve:dev",
    "build": "run-s clean cpx build:prod",
    "build:prod": "NODE_ENV=production webpack",
    "serve:dev": "NODE_ENV=development webpack-dev-server",
    "serve:prod": "NODE_ENV=production webpack-dev-server",
    "cpx": "run-s cpx:*",
    "cpx:jquery": "cpx ./node_modules/jquery/dist/jquery.min.js ./src/static/js -v",
    "cpx:@babel/polyfill": "cpx ./node_modules/@babel/polyfill/dist/polyfill.min.js ./src/static/js -v",
    "cpx:picturefill": "cpx ./node_modules/picturefill/dist/picturefill.min.js ./src/static/js -v",
    "clean": "rimraf ./build",
    "fix": "run-s fix:*",
    "fix:js": "eslint --fix ./src/**/*.js",
    "fix:scss": "stylelint --fix --ignore-path .lintignore '.src/**/*.scss'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "autoprefixer": "^9.4.7",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "copy-webpack-plugin": "^4.6.0",
    "cpx": "^1.5.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^3.0.1",
    "glob": "^7.1.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "imagemin-webpack-plugin": "^2.4.1",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.4",
    "pug": "^2.0.3",
    "pug-html-loader": "^1.1.5",
    "resolve-url-loader": "^3.0.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-config-sass-guidelines": "^5.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^2.0.0",
    "stylelint-scss": "^3.5.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "jquery": "^3.3.1",
    "picturefill": "^3.0.3"
  }
}
